<h3 class="ui header">{{t "forms.i-i-s-proba2-регист-анкеты-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-proba2-регист-анкеты-e.датаРегистр-caption"}}</label>
    {{flexberry-simpledatetime
      type="date"
      readonly=readonly
      value=model.датаРегистр
      class=(if (v-get validationModel "датаРегистр" "isInvalid") "error")
      data-test-i-i-s-proba2-регист-анкеты-e-датаРегистр=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "датаРегистр" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.фИОКандид
      class=(if (v-get validationModel "фИОКандид" "isInvalid") "error")
      label=(t "forms.i-i-s-proba2-регист-анкеты-e.фИОКандид-caption")
      data-test-i-i-s-proba2-регист-анкеты-e-фИОКандид=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "фИОКандид" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.серНомПаспорта
      class=(if (v-get validationModel "серНомПаспорта" "isInvalid") "error")
      label=(t "forms.i-i-s-proba2-регист-анкеты-e.серНомПаспорта-caption")
      data-test-i-i-s-proba2-регист-анкеты-e-серНомПаспорта=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "серНомПаспорта" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.иНН
      class=(if (v-get validationModel "иНН" "isInvalid") "error")
      label=(t "forms.i-i-s-proba2-регист-анкеты-e.иНН-caption")
      data-test-i-i-s-proba2-регист-анкеты-e-иНН=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "иНН" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.снилс
      class=(if (v-get validationModel "снилс" "isInvalid") "error")
      label=(t "forms.i-i-s-proba2-регист-анкеты-e.снилс-caption")
      data-test-i-i-s-proba2-регист-анкеты-e-снилс=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "снилс" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.номерТелефона
      class=(if (v-get validationModel "номерТелефона" "isInvalid") "error")
      label=(t "forms.i-i-s-proba2-регист-анкеты-e.номерТелефона-caption")
      data-test-i-i-s-proba2-регист-анкеты-e-номерТелефона=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "номерТелефона" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.элПочта
      class=(if (v-get validationModel "элПочта" "isInvalid") "error")
      label=(t "forms.i-i-s-proba2-регист-анкеты-e.элПочта-caption")
      data-test-i-i-s-proba2-регист-анкеты-e-элПочта=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "элПочта" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-proba2-регист-анкеты-e.вакантДолжн-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.вакантДолжн
      displayAttributeName="должность"
      autocomplete=true
      relationName="вакантДолжн"
      projection="ВакантДолжнL"
      title=(t "forms.i-i-s-proba2-регист-анкеты-e.вакантДолжн-caption")
      readonly=readonly
      componentName="вакантДолжнLookup"
      class=(if (v-get validationModel "вакантДолжн" "isInvalid") "error")
      data-test-i-i-s-proba2-регист-анкеты-e-вакантДолжн=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "вакантДолжн" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-proba2-регист-анкеты-e.организация-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.организация
      displayAttributeName="наименование"
      autocomplete=true
      relationName="организация"
      projection="ОрганизацияL"
      title=(t "forms.i-i-s-proba2-регист-анкеты-e.организация-caption")
      readonly=readonly
      componentName="организацияLookup"
      class=(if (v-get validationModel "организация" "isInvalid") "error")
      data-test-i-i-s-proba2-регист-анкеты-e-организация=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "организация" "message")}}
  </div>
</form>
